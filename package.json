{
  "name": "rollup-plugin-dirlist",
  "version": "0.1.0",
  "description": "Rollup plugin to allow import of a directory listing as an array of objects with name, sizek etc.",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "scripts": {
    "prebuild:config": "tsc --project config/tsconfig.json",
    "build:config": "",
    "prebuild:devtools": "npm run build:config",
    "build:devtools": "tsc --project devtools/tsconfig.json",
    "pretest": "npm run build && tsc --project src/__tests__",
    "test": "jest",
    "prebuild": "npm run build:devtools",
    "build": "rollup -c",
    "postbuild": "npm run build:docs",
    "clean": "rm -rf build lib docs",
    "prepublishOnly": "npm run clean && NODE_ENV=production npm run build",
    "build:docs": "GRAPHVIZ_DOT=\"$(which dot)\" typedoc -out docs/api src/"
  },
  "bin": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BobKerns/rollup-plugin-debug.git"
  },
  "keywords": [
    "template",
    "node",
    "typescript",
    "rollup"
  ],
  "author": "Bob Kerns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BobKerns/rollup-plugin-dirlist/issues"
  },
  "homepage": "https://github.com/BobKerns/rollup-plugin-dirlist#readme",
  "dependencies": {
    "micromatch": "^4.0.2",
    "wcsize": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^24.9.1",
    "@types/ramda": "^0.26.40",
    "jest": "^24.9.0",
    "jest-mock-proxy": "^3.0.0",
    "ramda": "^0.26.1",
    "rollup": "^1.31.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.21.2",
    "rollup-plugin-visualizer": "^1.1.1",
    "rollup-pluginutils": "^2.8.2",
    "ts-jest": "^24.3.0",
    "typedoc": "^0.16.4",
    "typedoc-plugin-external-module-name": "^3.0.0",
    "typedoc-plugin-mermaid": "=1.0.0",
    "typedoc-plugin-npm-externals": "^1.0.0",
    "typedoc-plugin-external-module-name": "^3.0.0",
    "typedoc-plugin-mermaid": "=1.0.0",
    "mermaid": "^8.4.6",
   "typescript": "^3.7.5"
  }
}
